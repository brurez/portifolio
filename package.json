{
  "name": "portifolio",
  "version": "1.0.0",
  "description": "Meu website contendo informações sobre mim e meus projetos",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brurez/portifolio.git"
  },
  "author": "Bruno de Rezende",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": "8.6.0",
    "npm": "5.5.1"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon src/index.js --exec \"node -r babel-register\"",
    "dev:client": "webpack --watch",
    "clean": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:server": "npm run clean && mkdir dist && babel src -s -D -d dist --ignore src/client",
    "build:client": "webpack -p",
    "test": "jest --watch"
  },
  "keywords": [
    "brurez",
    "portifolio",
    "resume"
  ],
  "dependencies": {
    "@sendgrid/mail": "^6.1.4",
    "axios": "^0.17.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.4.0",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "hbs": "^4.0.1",
    "jquery": "^3.2.1",
    "morgan": "^1.7.0",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.1",
    "typed.js": "^2.0.6",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-register": "^6.24.1",
    "dotenv": "^4.0.0",
    "jest": "^21.0.2",
    "nodemon": "^1.11.0",
    "reload": "^2.2.2",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/brurez/portifolio/issues"
  },
  "homepage": "https://brurez.herokuapp.com/",
  "directories": {
    "test": "test"
  }
}
